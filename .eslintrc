{
    "extends": [
        "react-app",
        "react-app/jest"
      ],
    "rules": {
        "no-unused-vars": "warn",
        "quotes": ["warn", "single"],
        "jsx-quotes": ["warn", "prefer-double"],
        "prefer-const": "error",
        "indent": ["warn", 2],
        "max-len": ["warn", {"code": 120}],
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["warn", "always"]
    }
}